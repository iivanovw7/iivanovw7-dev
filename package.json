{
    "name": "iivanovw7-dev",
    "version": "1.0.0",
    "description": "Personal website",
    "type": "module",
    "scripts": {
        "dev": "cargo watch -x run",
        "dev:css": "pnpm clean:css && node esbuild.config.css.mjs --watchmode",
        "build": "pnpm build:css && pnpm build:server",
        "build:server": "cargo build --release",
        "build:css": "pnpm clean:css && node esbuild.config.css.mjs",
        "build:run": "cargo run",
        "test:format": "prettier --write --ignore-unknown .",
        "test:stylelint": "stylelint \"styles/**/*.{css,scss}\"",
        "test": "pnpm run /^test:/",
        "clean:css": "rimraf assets/css/main.css",
        "prepare": "husky"
    },
    "keywords": [],
    "author": "iivanovw7",
    "license": "ISC",
    "dependencies": {
        "@alpinejs/morph": "3.13.5",
        "alpinejs": "3.13.5",
        "gsap": "3.12.5",
        "htmx.org": "1.9.10"
    },
    "devDependencies": {
        "@commitlint/cli": "19.5.0",
        "@commitlint/config-conventional": "19.5.0",
        "autoprefixer": "10.4.17",
        "cz-conventional-changelog": "^3.3.0",
        "esbuild": "0.24.0",
        "esbuild-postcss": "0.0.4",
        "esbuild-sass-plugin": "3.3.1",
        "husky": "9.1.6",
        "postcss": "8.4.47",
        "postcss-100vh-fix": "1.0.2",
        "postcss-dark-theme-class": "1.3.0",
        "postcss-preset-env": "10.0.5",
        "prettier": "3.2.5",
        "rimraf": "6.0.1",
        "sass": "1.79.4",
        "stylelint": "16.9.0",
        "stylelint-config-recess-order": "5.1.1",
        "stylelint-config-recommended-scss": "14.1.0",
        "stylelint-config-sass-guidelines": "12.1.0",
        "stylelint-order": "6.0.4",
        "stylelint-scss": "6.7.0"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
